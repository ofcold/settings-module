{
	"name": "ofcold/module-setting",
	"description": "The System setting.",
	"license": "MIT",
	"authors": [
		{
			"name": "Bill li",
			"email": "bill.li@ofcold.com"
		}
	],
	"require": {
		"php": "^8.0",
		"ext-swoole": "*"
	},
	"require-dev": {
		"mockery/mockery": "^1.3.1",
		"phpunit/phpunit": "^10.0"
	},
	"autoload": {
		"psr-4": {
			"Ofcold\\Module\\Setting\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ofcold\\Module\\SettingTests\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"laravel": {
			"providers": [
				"Ofcold\\Module\\Setting\\SettingModuleServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev"
}
